# Environment
ENVIRONMENT=development

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_KEY=your_supabase_service_role_key
SUPABASE_STORAGE_BUCKET=uploads

# Core Providers
CACHE_PROVIDER=redis
RATE_LIMIT_PROVIDER=redis
STORAGE_PROVIDER=supabase
EMAIL_PROVIDER=resend
SMS_PROVIDER=console
PAYMENT_PROVIDER=stripe
PUSH_NOTIFICATION_PROVIDER=onesignal
LOGGING_PROVIDER=console
MONITORING_PROVIDERS=console
ANALYTICS_PROVIDERS=internal

# Redis
REDIS_URL=redis://localhost:6379

# Stripe (demo keys for development)
STRIPE_SECRET_KEY=sk_test_xxxxxxxxxxxxx
STRIPE_WEBHOOK_SECRET=whsec_xxxxxxxxxxxxx
STRIPE_PUBLISHABLE_KEY=pk_test_xxxxxxxxxxxxx

# Resend
RESEND_API_KEY=re_xxxxxxxxxxxxx

# OneSignal
ONESIGNAL_APP_ID=xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx
ONESIGNAL_API_KEY=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Email fallback (optional if using another provider)
EMAIL_FROM=noreply@yourdomain.com

# JWT
JWT_SECRET_KEY=your_jwt_secret_key_here
JWT_ALGORITHM=HS256
JWT_EXPIRATION_MINUTES=60

# Docker Registry (optional)
DOCKER_REGISTRY_URL=your_docker_registry_url
DOCKER_REGISTRY_TOKEN=your_docker_registry_token

# API URLs
API_URL=http://localhost:8000
FRONTEND_URL=http://localhost:3000

# CORS Origins (JSON array or comma-separated string)
CORS_ORIGINS=["http://localhost:3000","http://127.0.0.1:3000"]
